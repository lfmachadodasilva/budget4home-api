{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
    "ConnectionStrings": {
        "Heroku": "Server=$HEROKU_DB_HOST;Port=5432;Database=$HEROKU_DB_DATABASE;User Id=$HEROKU_DB_USER;Password=$HEROKU_DB_PASSWORD;SSL Mode=Prefer;Trust Server Certificate=true;",
        "Redis": "$HEROKU_REDIS"
    },
  "AppConfig": {
    "BuildVersion": "$GITHUB_RUN_NUMBER",
    "FirebaseAdmin": "$FIREBASE_ADMIN",
    "ConnectionString": "Heroku"
  },
  "Firebase": {
    "ProjectId": "$FIREBASE_PROJECT",
    "WebApiKey": "$FIREBASE_API_KEY"
  }
}
